library(methods)
source("strategico_util.R")

args <- commandArgs()

project.name <- args[6]
id.min <- args[7]
id.max <- args[8]
item.values.string <- args[9]
item.params <- args[10]

item.values <- unlist(strsplit(item.values.string, ","))

msg <- paste("Project=", project.name, ", ID min=", id.min, ", ID max=", id.max, sep="")
print(msg)
msg <- paste("item.values=", item.values.string, ", item.params=", item.params, sep="")
print(msg)

project.config <- GetProjectConfig(project.name)
param <- EvalParamString(item.params)

EvalItems(project.name=project.name, 
          id.min=id.min, id.max=id.max,
          values=item.values, param=param, project.config=project.config)
